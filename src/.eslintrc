{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules":{
        "semi": "off",
        "require-jsdoc": "off",
        "no-trailing-spaces": "off",
        "no-debugger": "off",
        "no-unused-vars": "off"

    },
    "extends": ["eslint:recommended", "google"]
}